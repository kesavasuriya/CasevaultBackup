<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Received_Date_should_not_be_future_date</fullName>
    <active>true</active>
    <errorConditionFormula>ISNEW()  &amp;&amp; Received_Date__c  &gt;   NOW()</errorConditionFormula>
    <errorDisplayField>Received_Date__c</errorDisplayField>
    <errorMessage>Date or Time should not be in future.</errorMessage>
</ValidationRule>
