<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Zip_Code_Format</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(Zipcode__c, &quot;\\d{5}(-\\d{4})?&quot;))</errorConditionFormula>
    <errorDisplayField>Zipcode__c</errorDisplayField>
    <errorMessage>Zip code must be in the 22222-1111 format.</errorMessage>
</ValidationRule>
