<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CPA_Office_Id</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL( Type__c , &apos;CPA Home&apos;),ISBLANK(CPA_Office_ID__c))</errorConditionFormula>
    <errorDisplayField>CPA_Office_ID__c</errorDisplayField>
