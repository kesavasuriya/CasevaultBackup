<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Application_packet_not_future_date</fullName>
    <active>true</active>
    <errorConditionFormula>Application_Packed_Sent_Date__c &gt; TODAY()</errorConditionFormula>
    <errorDisplayField>Application_Packed_Sent_Date__c</errorDisplayField>
    <errorMessage>Application Packed Sent Date cannot be a future date</errorMessage>
</ValidationRule>
